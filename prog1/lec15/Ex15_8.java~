class Ex15_8{
    public static void main(String[] args){
	int [][] n={{2,7},{6,9,5,1},{4,3,8}};
	calcAve(n);
    }
    public static double calcAve(int n){
	return n[i]=n[0][0]+n[0][1]/2;
    return n[i]=n[1][0]+n[1][1]+n[1][5]+n[1][3]/4;
    return n[i]=n[2][0]+n[2][1]+n[2][2]/3;
    }
    System.out.println(n[i]);
}
