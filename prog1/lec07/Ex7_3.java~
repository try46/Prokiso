class Ex7_3{
    public static void main(String[] args){
	int a = 15;
	char range;
	switch(a){
	case 0:
	case 1:
	case 2:
	case 3:
	case 4:
	case 5:
	case 6:
	range="A";
	break;
	case 7:
	case 8:
	case 9:
	case 10:
	case 11:
	case 12:
	case 13:
	range = "B";
	    break;
	case 14:
	case 15:
	case 16:
	case 17:
	case 18:
	case 19:
	case 20:
	    range = "C";
	    break;
	default:
	    range="D";
    break;
    System.out.println(range);
	}
    }
}


	
